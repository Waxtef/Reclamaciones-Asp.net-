<body>

    <div class="wrapper">

        <div class="top_navbar">
            
            <div class="top_menu">
                <div class="logo">
                    
                    @Html.ActionLink("Streek", "Index", "Home", new { area = "" }, new { @class = "logo" })
                </div>
                <ul>
                    <li>
                        <a href="#">
                            <i class="fas fa-user-alt"></i>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="fas fa-sign-out-alt">@Html.ActionLink("Log", "Login", "Account")</i>
                        </a>
                    </li>
                </ul>
            </div>
        </div>

        <div class="sidebar">
            <ul>
                <li>
                    @Html.ActionLink("Usuarios", "Index", "Usuarios")
                </li>
                <li>

                    <!--<span class="icon"><i class="fas fa-list-alt"></i></span>-->
                    @Html.ActionLink("Tipo Reclamacion", "Index", "Tipo_Reclamacion")

                </li>
                <li>

                    @Html.ActionLink("Categorias", "Index", "Categorias")

                </li>
                <li>

                    @Html.ActionLink("Observador", "Index", "Observadors")

                </li>
                <li>

                    @Html.ActionLink("Reclamaciones", "Index", "Reclamacions")

                </li>
                <li>

                    @Html.ActionLink("About", "About", "Home")

                </li>
            </ul>
           
        </div>

        <div class="main_container">
            <div class="item">
                @RenderBody()
            </div>
           
        </div>
        <div class="container body-content">
        
    </div>
   
    </div>










